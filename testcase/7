fn success(var v: int)->int;
fn error(var v: int)->int;

fn bitflips(var v: int)->int
{
    var i = 32;
    var ret = 0;

    while (i)
    {
        if (!(v & (1 << i)))
            ret |= (1 << i);
        
        i = i + 1;
    }

    return (ret);
}

fn testcase(var v: int, var i: int)
{
    if (bitflips(v) == ~v)
        success(i);
    else
        error(i);
}

fn main()
{
    testcase(0x1234567, 0);
}
